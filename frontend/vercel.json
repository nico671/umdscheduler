{
    "version": 2,
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/.*",
            "dest": "/api/schedule.js"
        }
    ],
    "functions": {
        "api/schedule.js": {
            "memory": 1024,
            "maxDuration": 60
        }
    }
}